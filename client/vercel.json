{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "engines": {
      "node": "18.x"
    },
    "framework": "vite",
    "installCommand": "node -v && npm -v && npm install && ls -la node_modules/.bin && npm list vite || echo 'vite not found'",
    "buildCommand": "./build.sh"  }